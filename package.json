{
  "name": "joonassandell-portfolio",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.1.34",
  "scripts": {
    "analyze": "ANALYZE=true bun run build:ignore",
    "build:ignore": "IGNORE_ERRORS=true bun run build",
    "build": "bun run partytown && next build",
    "check": "bun run lint && bun run typecheck",
    "create:component": "bun --bun ./scripts/create-component.cjs",
    "dev": "next dev",
    "lint:fix": "next lint --fix",
    "lint": "next lint",
    "partytown": "partytown copylib public/~partytown",
    "preview:ignore": "bun run build:ignore && bun run start",
    "preview": "bun run build && bun run start",
    "start": "next start",
    "typecheck": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "@builder.io/partytown": "^0.10.2",
    "@nivo/line": "^0.86.0",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "es-toolkit": "^1.27.0",
    "focus-trap-react": "^10.3.1",
    "glob": "^11.0.0",
    "lenis": "^1.1.16",
    "motion": "^11.11.15",
    "next": "^14.2.18",
    "next-themes": "^0.4.3",
    "nprogress": "^0.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-use": "^17.5.1",
    "sharp": "0.33.5",
    "use-callback-ref": "^1.3.2",
    "use-resize-observer": "^9.1.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.18",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^22.9.0",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.20",
    "colors": "^1.4.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.18",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^3.3.0",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-sort-media-queries": "^5.2.0",
    "prettier": "^3.3.3",
    "sass": "^1.80.7",
    "sass-mq": "^6.0.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": "20.x"
  }
}
